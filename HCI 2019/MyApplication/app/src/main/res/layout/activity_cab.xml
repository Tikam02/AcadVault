<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">



    <RelativeLayout
        android:id="@+id/rectangle_1"
        android:layout_width="410dp"
        android:layout_height="57dp"
        android:background="#3F51B5"
        android:layout_alignParentTop="true">

        <RelativeLayout
            android:id="@+id/rel_1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <View
                android:id="@+id/rectangle_2"
                android:layout_width="90dp"
                android:layout_height="57dp"
                android:background="@drawable/roundbtn"
                android:elevation="4dp" />

            <View
                android:id="@+id/bt_2"
                android:layout_width="57dp"
                android:layout_height="57dp"
                android:background="@drawable/star"
                android:elevation="4dp"
                android:layout_marginLeft="16dp" />

        </RelativeLayout>


        <RelativeLayout
            android:id="@+id/rel_2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toEndOf="@+id/rel_1"
            android:layout_marginStart="16dp">

            <View
                android:id="@+id/rectangle_3"
                android:layout_width="90dp"
                android:layout_height="57dp"
                android:background="@drawable/roundbtn"
                android:elevation="4dp"
                />

            <View
                android:id="@+id/bt_3"
                android:layout_width="45dp"
                android:layout_height="40dp"
                android:background="@drawable/heart"
                android:elevation="4dp"
                android:layout_marginStart="22dp"
                android:layout_marginTop="8dp"/>
        </RelativeLayout>
        <!-- Rectangle 17 -->


        <RelativeLayout
            android:id="@+id/rel_3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toEndOf="@+id/rel_2"
            android:layout_marginStart="16dp">
            <View
                android:id="@+id/rectangle_4"
                android:layout_width="90dp"
                android:layout_height="57dp"
                android:background="@drawable/roundbtn"
                android:elevation="4dp" />

            <View
                android:id="@+id/bt_4"
                android:layout_width="57dp"
                android:layout_height="57dp"
                android:background="@drawable/bullet"
                android:elevation="4dp"
                android:layout_marginStart="22dp"
                />

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rel_4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toEndOf="@+id/rel_3"
            android:layout_marginStart="17dp">
            <View
                android:id="@+id/rectangle_5"
                android:layout_width="90dp"
                android:layout_height="57dp"
                android:background="@drawable/roundbtn"
                android:elevation="4dp" />

            <View
                android:id="@+id/bt_5"
                android:layout_width="57dp"
                android:layout_height="57dp"
                android:background="@drawable/add"
                android:elevation="4dp"
                android:layout_marginStart="18dp"/>
        </RelativeLayout>
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="350dp"
        android:layout_height="300dp"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="20dp"
        android:layout_below="@+id/rectangle_1"
        android:id="@+id/map">

        <View
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@drawable/map" />


    <GridLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:id="@+id/location"
        android:layout_centerHorizontal="true"
        android:background="#D3D3D3">

        <View
            android:layout_width="20dp"
            android:layout_marginTop="10dp"
            android:layout_height="25dp"
            android:layout_marginLeft="10dp"
            android:background="@drawable/blue_marker"
            android:layout_row="1"
            android:layout_column="1"/>
        <TextView
            android:layout_width="200dp"
            android:layout_marginLeft="3dp"
            android:layout_height="25dp"
            android:layout_marginTop="10dp"
            android:layout_marginRight="10dp"
            android:background="@color/grey"
            android:textColor="@color/black"
            android:textSize="6pt"
            android:textStyle="bold"
            android:gravity="center_vertical|center_horizontal"
            android:text="Infocity, near Kudasan"
            android:layout_row="1"
            android:layout_column="2"/>

        <View
            android:layout_marginTop="10dp"
            android:layout_marginLeft="10dp"
            android:layout_width="20dp"
            android:layout_marginBottom="10dp"
            android:layout_height="25dp"
            android:background="@drawable/red_marker"
            android:layout_row="2"
            android:layout_column="1"/>
        <TextView
            android:layout_marginTop="10dp"
            android:layout_marginRight="10dp"
            android:layout_width="200dp"
            android:layout_marginLeft="3dp"
            android:gravity="center_vertical|center_horizontal"
            android:layout_height="25dp"
            android:background="@color/grey"
            android:textColor="@color/black"
            android:textSize="6pt"
            android:layout_marginBottom="10dp"
            android:textStyle="bold"
            android:text="DAIICT, near Kudasan"
            android:layout_row="2"
            android:layout_column="2"/>

    </GridLayout>

    </RelativeLayout>

    <Spinner
        android:layout_marginTop="20dp"
        android:layout_height="50dp"
        android:layout_width="200dp"
        android:id="@+id/cab_list123"
        android:layout_below="@+id/map"
        android:layout_centerHorizontal="true"
        />

    <GridLayout
        android:layout_width="350dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/cab_list123"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="20dp">
        <TextView
            android:layout_height="40dp"
            android:width="115dp"
            android:text="PRICE"
            android:textStyle="bold"
            android:background="@drawable/roundbtn"
            android:backgroundTint="#00ffff"
            android:textColor="@color/black"
            android:gravity="center_horizontal|center_vertical"
            android:layout_row="1"
            android:layout_column="0"/>

        <TextView
            android:layout_height="40dp"
            android:width="115dp"
            android:text="APP"
            android:layout_marginStart="120dp"
            android:textStyle="bold"
            android:background="@drawable/roundbtn"
            android:backgroundTint="#F98B88"
            android:textColor="@color/black"
            android:gravity="center_horizontal|center_vertical"
            android:layout_row="1"
            android:layout_column="1"/>

        <!-- 1st Item -->

        <TextView
            android:layout_height="40dp"
            android:width="115dp"
            android:text="₹ 17"
            android:layout_marginTop="30dp"
            android:textStyle="bold"
            android:background="@drawable/roundbtn"
            android:backgroundTint="#b351f7"
            android:textSize="10pt"
            android:textColor="@color/black"
            android:gravity="center_horizontal|center_vertical"
            android:layout_row="3"
            android:layout_column="0"/>

        <ImageView
            android:layout_height="40dp"
            android:layout_width="115dp"
            android:layout_marginStart="120dp"
            android:background="@drawable/ola"
            android:layout_marginTop="30dp"
            android:layout_row="3"
            android:layout_column="1"/>

        <!-- 1st Item -->

        <TextView
            android:layout_height="40dp"
            android:width="115dp"
            android:text="₹ 17"
            android:layout_marginTop="10dp"
            android:textStyle="bold"
            android:textSize="10pt"
            android:background="@drawable/roundbtn"
            android:backgroundTint="#b351f7"
            android:textColor="@color/black"
            android:gravity="center_horizontal|center_vertical"
            android:layout_row="4"
            android:layout_column="0"/>

        <ImageView
            android:layout_height="40dp"
            android:layout_width="115dp"
            android:layout_marginStart="120dp"
            android:background="@drawable/uber"
            android:layout_marginTop="10dp"
            android:layout_row="4"
            android:layout_column="1"/>


        <!-- 1st Item -->

        <TextView
            android:layout_height="40dp"
            android:width="115dp"
            android:text="₹ 17"
            android:layout_marginTop="10dp"
            android:textStyle="bold"
            android:background="@drawable/roundbtn"
            android:backgroundTint="#b351f7"
            android:textColor="@color/black"
            android:textSize="10pt"
            android:gravity="center_horizontal|center_vertical"
            android:layout_row="5"
            android:layout_column="0"/>

        <ImageView
            android:layout_height="40dp"
            android:layout_width="115dp"
            android:layout_marginStart="120dp"
            android:background="@drawable/jugnu"
            android:layout_marginTop="10dp"
            android:layout_row="5"
            android:layout_column="1"/>

    </GridLayout>


    <RelativeLayout
        android:id="@+id/rectangle_7"
        android:layout_width="410dp"
        android:layout_height="57dp"
        android:layout_alignParentBottom="true"
        android:background="#3F51B5">

        <RelativeLayout
            android:id="@+id/rel_5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <View
                android:id="@+id/rectangle_8"
                android:layout_width="90dp"
                android:layout_height="57dp"
                android:background="@drawable/roundbtn"
                android:elevation="4dp" />

            <View
                android:id="@+id/bt_6"
                android:layout_width="57dp"
                android:layout_height="57dp"
                android:background="@drawable/ic_phone"
                android:backgroundTint="#00ff00"
                android:elevation="4dp"
                android:layout_marginLeft="16dp" />

        </RelativeLayout>


        <RelativeLayout
            android:id="@+id/rel_6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toEndOf="@+id/rel_5"
            android:layout_marginStart="16dp">

            <View
                android:id="@+id/rectangle_9"
                android:layout_width="90dp"
                android:layout_height="57dp"
                android:background="@drawable/roundbtn"
                android:backgroundTint="#FA8072"
                android:elevation="4dp"
                />

            <View
                android:id="@+id/bt_7"
                android:layout_width="57dp"
                android:layout_height="57dp"
                android:background="@drawable/cab"
                android:backgroundTint="#ffff00"
                android:elevation="4dp"
                android:layout_marginStart="17dp"/>
        </RelativeLayout>
        <!-- Rectangle 17 -->


        <RelativeLayout
            android:id="@+id/rel_7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toEndOf="@+id/rel_6"
            android:layout_marginStart="16dp">
            <View
                android:id="@+id/rectangle_10"
                android:layout_width="90dp"
                android:layout_height="57dp"
                android:background="@drawable/roundbtn"
                android:elevation="4dp" />

            <View
                android:id="@+id/bt_8"
                android:layout_width="57dp"
                android:layout_height="57dp"
                android:background="@drawable/home"
                android:backgroundTint="#550000"
                android:elevation="4dp"
                android:layout_marginStart="16dp"
                />

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rel_8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toEndOf="@+id/rel_7"
            android:layout_marginStart="17dp">
            <View
                android:id="@+id/rectangle_11"
                android:layout_width="90dp"
                android:layout_height="57dp"
                android:background="@drawable/roundbtn"
                android:elevation="4dp" />

            <View
                android:id="@+id/bt_9"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:background="@drawable/ent"
                android:elevation="4dp"
                android:layout_marginStart="20dp"
                android:layout_marginTop="3dp"/>
        </RelativeLayout>
    </RelativeLayout>

</RelativeLayout>