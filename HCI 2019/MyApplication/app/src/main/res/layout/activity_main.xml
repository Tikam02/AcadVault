<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">



<RelativeLayout
    android:id="@+id/rectangle_1"
    android:layout_width="410dp"
    android:layout_height="57dp"
    android:background="#3F51B5"
    android:layout_alignParentTop="true">

    <RelativeLayout
        android:id="@+id/rel_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">

        <View
            android:id="@+id/rectangle_2"
            android:layout_width="90dp"
            android:layout_height="57dp"
            android:background="@drawable/roundbtn"
            android:elevation="4dp" />

        <View
            android:id="@+id/bt_2"
            android:layout_width="57dp"
            android:layout_height="57dp"
            android:background="@drawable/star"
            android:elevation="4dp"
            android:layout_marginLeft="16dp" />

    </RelativeLayout>


<RelativeLayout
    android:id="@+id/rel_2"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_toEndOf="@+id/rel_1"
    android:layout_marginStart="16dp">

    <View
        android:id="@+id/rectangle_3"
        android:layout_width="90dp"
        android:layout_height="57dp"
        android:background="@drawable/roundbtn"
        android:elevation="4dp"
        />

    <View
        android:id="@+id/bt_3"
        android:layout_width="45dp"
        android:layout_height="40dp"
        android:background="@drawable/heart"
        android:elevation="4dp"
        android:layout_marginStart="22dp"
        android:layout_marginTop="8dp"/>
</RelativeLayout>
    <!-- Rectangle 17 -->


<RelativeLayout
    android:id="@+id/rel_3"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_toEndOf="@+id/rel_2"
    android:layout_marginStart="16dp">
    <View
        android:id="@+id/rectangle_4"
        android:layout_width="90dp"
        android:layout_height="57dp"
        android:background="@drawable/roundbtn"
        android:elevation="4dp" />

    <View
        android:id="@+id/bt_4"
        android:layout_width="57dp"
        android:layout_height="57dp"
        android:background="@drawable/bullet"
        android:elevation="4dp"
        android:layout_marginStart="22dp"
        />

</RelativeLayout>

    <RelativeLayout
        android:id="@+id/rel_4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toEndOf="@+id/rel_3"
        android:layout_marginStart="17dp">
        <View
            android:id="@+id/rectangle_5"
            android:layout_width="90dp"
            android:layout_height="57dp"
            android:background="@drawable/roundbtn"
            android:elevation="4dp" />

        <View
            android:id="@+id/bt_5"
            android:layout_width="57dp"
            android:layout_height="57dp"
            android:background="@drawable/add"
            android:elevation="4dp"
            android:layout_marginStart="18dp"/>
    </RelativeLayout>
</RelativeLayout>

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:id="@+id/bar">

        <RelativeLayout
            android:id="@+id/rectangle_6"
            android:layout_width="360dp"
            android:layout_height="80dp"
            android:background="@drawable/roundbtn">

        <Button
            android:id="@+id/button"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:background="@drawable/keyboard"
            android:layout_marginLeft="20dp"
            android:layout_centerVertical="true"/>

        <Button
            android:id="@+id/button2"
            android:layout_toRightOf="@+id/button"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:background="@drawable/ic_mic"
            android:layout_marginLeft="200dp"
            android:layout_centerVertical="true"/>
        </RelativeLayout>

    </RelativeLayout>

    <TextView
        android:layout_width="360dp"
        android:layout_height="200dp"
        android:layout_below="@+id/bar"
        android:background="@drawable/roundbtn"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="20dp"
        android:textSize="14pt"
        android:id="@+id/command"
        android:textColor="#fff"
        />


    <RelativeLayout
        android:id="@+id/rectangle_7"
        android:layout_width="410dp"
        android:layout_height="57dp"
        android:layout_alignParentBottom="true"
        android:background="#3F51B5">

        <RelativeLayout
            android:id="@+id/rel_5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <View
                android:id="@+id/rectangle_8"
                android:layout_width="90dp"
                android:layout_height="57dp"
                android:background="@drawable/roundbtn"
                android:elevation="4dp" />

            <View
                android:id="@+id/bt_6"
                android:layout_width="57dp"
                android:layout_height="57dp"
                android:background="@drawable/ic_phone"
                android:backgroundTint="#00ff00"
                android:elevation="4dp"
                android:layout_marginLeft="16dp" />

        </RelativeLayout>


        <RelativeLayout
            android:id="@+id/rel_6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toEndOf="@+id/rel_5"
            android:layout_marginStart="16dp">

            <View
                android:id="@+id/rectangle_9"
                android:layout_width="90dp"
                android:layout_height="57dp"
                android:background="@drawable/roundbtn"
                android:elevation="4dp"
                />

            <View
                android:id="@+id/bt_7"
                android:layout_width="57dp"
                android:layout_height="57dp"
                android:background="@drawable/cab"
                android:backgroundTint="#ffff00"
                android:elevation="4dp"
                android:layout_marginStart="17dp"/>
        </RelativeLayout>
        <!-- Rectangle 17 -->


        <RelativeLayout
            android:id="@+id/rel_7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toEndOf="@+id/rel_6"
            android:layout_marginStart="16dp">
            <View
                android:id="@+id/rectangle_10"
                android:layout_width="90dp"
                android:layout_height="57dp"
                android:background="@drawable/roundbtn"
                android:elevation="4dp" />

            <View
                android:id="@+id/bt_8"
                android:layout_width="57dp"
                android:layout_height="57dp"
                android:background="@drawable/home"
                android:backgroundTint="#550000"
                android:elevation="4dp"
                android:layout_marginStart="16dp"
                />

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rel_8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toEndOf="@+id/rel_7"
            android:layout_marginStart="17dp">
            <View
                android:id="@+id/rectangle_11"
                android:layout_width="90dp"
                android:layout_height="57dp"
                android:background="@drawable/roundbtn"
                android:elevation="4dp" />

            <View
                android:id="@+id/bt_9"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:background="@drawable/ent"
                android:elevation="4dp"
                android:layout_marginStart="20dp"
                android:layout_marginTop="3dp"/>
        </RelativeLayout>
    </RelativeLayout>





</RelativeLayout>