.orig x20
.start x20
.clear

MOV W x257 R5	//R5 INITIAL NUMBER
MOV W R5 R1	
MOV W X1 R3	//R3=COUNTER FOR MULTIPLY
AND W XF00 R1
SHIFT W,RIGHT,NC R1 X8
MOV W R1 R2
MOV W X64 R7	//R7=MULTIPLICATION FACTOR
CALL MULTIPLY
MOV W R1 R10

MOV W R5 R1
MOV W X1 R3
AND W XF0 R1
SHIFT W,RIGHT,NC R1 X4
MOV W R1 R2
MOV W X0A R7
CALL MULTIPLY
MOV W R1 R11

MOV W R5 R1
MOV W X1 R3
AND W XF R1
MOV W R1 R12

MOV W R10 R8
ADD W R11 R8
ADD W R12 R8
HALT
MULTIPLY:	ADD W R2 R1
		ADD W X1 R3
		COMP W R7 R3
		JGT MULTIPLY
		RET      
.end