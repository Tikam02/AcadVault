.ORIG X04
OUT #2 R5
RET

.ORIG X400
.START X400
IN #0 R1
IN #1 R2
MOV B #1 R3
MOV B #1 R4
MOV B #0 R5
MOV B #0 R8

LOOP: MOV B #1 R3
MOV B #1 R4
ADD B #1 R8
COMP B #8 R8
JLT ANS
AND B R1 R3
AND B R2 R4
SHIFT B R NC R1 #1
SHIFT B R NC R2 #1
XOR B R3 R4
COMP B #1 R4
JEQ COUNT
JMP LOOP
HALT
COUNT: ADD B #1 R5
JMP LOOP
HALT
ANS: MOV B #22 R12
ADD B #1 R12
JMP ANS 
.END